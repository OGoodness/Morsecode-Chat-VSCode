{
  "name": "morsecode-chat",
  "publisher": "OGoodness",
  "displayName": "MorseCode Chat",
  "author": "Noah Fields",
  "icon": "media/morsecode.png",
  "description": "MorseCode Chat for VSCode",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/OGoodness/morsecode"
  },
  "engines": {
    "vscode": "^1.51.0"
  },
  "keywords": [
    "morse",
    "morsecode",
    "chat",
    "message",
    "communicate",
    "fun"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:morsecode-sidebar",
    "onCommand:morsecode.reloadSidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "morsecode-sidebar-view",
          "title": "Morsecode",
          "icon": "media/morsecode.png"
        }
      ]
    },
    "views": {
      "morsecode-sidebar-view": [
        {
          "type": "webview",
          "id": "morsecode-sidebar",
          "name": "Morsecode",
          "icon": "media/morsecode.png",
          "contextualTitle": "Morsecode"
        }
      ]
    },
    "commands": [
      {
        "command": "morsecode.reloadSidebar",
        "title": "Reload Sidebar",
        "category": "Morsecode"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "rollup -c && webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/node-fetch": "^2.5.7",
    "@types/vscode": "^1.51.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "reconnecting-websocket": "^4.4.0"
  }
}
